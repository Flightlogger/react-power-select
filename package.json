{
  "name": "react-power-select",
  "version": "1.0.4",
  "description": "A highly composable & reusable select component in react",
  "main": "lib/index.js",
  "scripts": {
    "start": "webpack-dev-server --config docs/app/webpack.config.js --content-base docs/index.html",
    "clean": "rm -rf lib",
    "build:commonjs": "babel src/ --out-dir lib/",
    "build:docs": "webpack --config docs/app/webpack.config.js --env production",
    "build": "npm run clean && npm run build:commonjs && webpack && rm -rf dist/css.js",
    "prepublish": "npm run build",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --single-quote --trailing-comma es5 --write",
      "git add"
    ],
    "docs/app/Demos/**/*.js": [
      "prettier --single-quote --trailing-comma es5 --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/selvagsz/react-power-select.git"
  },
  "author": "selvagsz (selvaganeshbsg@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/selvagsz/react-power-select/issues"
  },
  "keywords": [
    "react-power-select",
    "react-autocomplete",
    "react-select",
    "react-typeahead",
    "react-multiselect",
    "react-component"
  ],
  "files": [
    "lib",
    "dist"
  ],
  "homepage": "https://github.com/selvagsz/react-power-select#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "codemirror": "^5.27.2",
    "css-loader": "^0.28.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.4",
    "lint-staged": "^3.6.1",
    "minimist": "^1.2.0",
    "multi-loader": "^0.1.0",
    "node-sass": "^4.5.3",
    "prettier": "^1.4.4",
    "react": "next",
    "react-codemirror": "^1.0.0",
    "react-dom": "next",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "webpack": "^2.6.0",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react-tether": "^0.5.7"
  }
}
